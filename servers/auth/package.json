{
  "name": "auth",
  "version": "2.0.0",
  "description": "Authentication server for Go Places. Will load the initial page for web application",
  "main": "sw.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,./package-lock.json --copy-files",
    "start": "npm run build && node --require dotenv/config dist/server.js",
    "empty_database": "node --require dotenv/config src/models/init.db dropTables",
    "create_database": "node --require dotenv/config src/models/init.db createTables"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.1.0",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "moment": "^2.24.0",
    "pg": "^7.12.1",
    "rimraf": "^3.0.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "express": "^4.16.2",
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0"
  }
}
